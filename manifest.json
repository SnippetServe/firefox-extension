{
  "description": "Checks snippets on getcode",
  "manifest_version": 2,
  "name": "Snippet Collector",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "icons":{
    "48": "icons/border-48.png"
  },
  "background":{
    "scripts":["popup.js"]
  },
  "browser_action": {
    "default_title": "Snippet Search",
    "default_popup": "popup.html"
  }
}
